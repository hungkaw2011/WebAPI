@model IEnumerable<WebApp.UI.Models.DTO.RegionDto>

<div class="container">
    <div class="mt-3">
        <table class="table table-bordered table-striped" style="width:100%">
            <thead>
                <tr>
                    <th>
                        Id
                    </th>
                    <th>
                        Name
                    </th>
                    <th>
                        RegionImageUrl
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var obj in Model)
                {
                    <tr>
                        <td width="50%">
                            @obj.Id
                        </td>
                        <td width="30%">
                            @obj.Name
                        </td>
                        <td>
                            @obj.RegionImageUrl
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
